proc read_sing_dbm
{ 
  link l="DBM: NFlist";
  read(l, "A[k]");
  read(l, "D[k]");
  read(l, "E[6k]");
  read(l, "E[6k+1]");
  read(l, "E[6k+2]");
  read(l, "J[k,0]");
  read(l, "J[k,r]");
  read(l, "X[1,0]");
  read(l, "X[1,r]");
  read(l, "X[k,0]");
  read(l, "X[k,r]");
  read(l, "W[12k]");
  read(l, "W[12k+1]");
  read(l, "W[12k+5]");
  read(l, "W[12k+6]");
  read(l, "W[k,0]");
  read(l, "W[k,r]");
  read(l, "W#[k,2r-1]");
  read(l, "W#[k,2r]");
  read(l, "Y[1,r,s]");
  read(l, "Y[k,r,s]");
  read(l, "Z[1,0]");
  read(l, "Z[1,r]");
  read(l, "Z[k,r,s]");
  read(l, "Z[k,r,0]");
  read(l, "Z[k,12k+6r-1]");
  read(l, "Z[k,12k+6r]");
  read(l, "Z[k,12k+6r+1]");
  read(l, "Z[k,0]");
  read(l, "Z[k,r]");
  read(l, "Z[6k+5]");
  read(l, "Z[6k+6]");
  read(l, "Z[6k+7]");
  read(l, "Q[k,0]");
  read(l, "Q[k,r]");
  read(l, "Q[6k+4]");
  read(l, "Q[6k+5]");
  read(l, "Q[6k+6]");
  read(l, "S[12k-1]");
  read(l, "S[12k]");
  read(l, "S[k,0]");
  read(l, "S[k,r]");
  read(l, "S#[k,2r-1]");
  read(l, "S#[k,2r]");
  read(l, "S[12k+4]");
  read(l, "S[12k+5]");
  read(l, "U[12k]");
  read(l, "U[k,2r-1]");
  read(l, "U[k,2r]");
  read(l, "U[12k+4]");
  read(l, "V[1,0]");
  read(l, "V[1,r]");
  read(l, "V#[1,2r-1]");
  read(l, "V#[1,2r]");
  read(l, "T[k,r,s]");
  close(l);
}
